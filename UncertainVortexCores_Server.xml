<ServerManagerConfiguration>
    <ProxyGroup name="filters">
        <!-- ================================================================== -->
        <SourceProxy name="UncertainVortexCores" class="UncertainVortexCores" label="Uncertain Vortex Cores">
            <Documentation
                long_help="Computes Vortex Cores of Uncertain Vectorfields."
                short_help="Computes Vortex Cores.">
            </Documentation>

            <InputProperty
                name="Input"
                port_index="0"
                command="SetInputConnection">
                <ProxyGroupDomain name="groups">
                    <Group name="sources"/>
                    <Group name="filters"/>
                </ProxyGroupDomain>
                <DataTypeDomain name="input_type">
                    <DataType value="vtkImageData"/>
                </DataTypeDomain>
                <InputArrayDomain name="input_array1" attribute_type="point" number_of_components="3">
                    <RequiredProperties>
                        <Property name="SelectInputVectors" function="FieldDataSelection"/>
                    </RequiredProperties>
                </InputArrayDomain>
            </InputProperty>

            <IntVectorProperty
                name="Number Samples"
                command="SetnumSamples"
                number_of_elements="1"
                default_values="20">
                <Documentation>
                    Number of Monte-Carlo samples for each cell.
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty
                name="Seed"
                command="Setseed"
                number_of_elements="1"
                default_values="42">
                <Documentation>
                    Seed for the generation of the Monte-Carlo samples.
                </Documentation>
            </IntVectorProperty>

            <IntVectorProperty
                name="Decomposition"
                command="SetdecompType"
                number_of_elements="1"
                default_values="1">
                <EnumerationDomain name="enum">
                    <Entry value="0" text="Cholesky"/>
                    <Entry value="1" text="Eigendecomp"/>
                </EnumerationDomain>
                <Documentation>
                    Decomposition used for the generation of the Monte-Carlo samples.
                </Documentation>
            </IntVectorProperty>

            <OutputPort name="Vortex Core Lines" index="0" id="port0"/>

        </SourceProxy>
        <!-- End Reader -->
    </ProxyGroup>
    <!-- End Filters Group -->
</ServerManagerConfiguration>
